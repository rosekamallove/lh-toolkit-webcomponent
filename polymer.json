{
  "entrypoint": "index.html",
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/*.js",
    "!node_modules/@webcomponents/webcomponentsjs/gulpfile.js",
    "node_modules/@webcomponents/webcomponentsjs/bundles/*.js"
  ],
  "fragments": [
    "index.html",
    "demos/fhir-active-status.html",
    "demos/fhir-birth-date.html",
    "demos/fhir-decease-status.html",
    "demos/fhir-human-address.html",
    "demos/fhir-human-contact.html",
    "demos/fhir-human-gender.html",
    "demos/fhir-human-language.html",
    "demos/fhir-human-name.html",
    "demos/fhir-marital-status.html",
    "demos/fhir-period.html",
    "demos/fhir-person-identifier.html"
  ],
  "builds": [
    {
      "name": "esm-bundled",
      "browserCapabilities": [
        "es2015",
        "modules"
      ],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true,
  "lint": {
    "rules": [
      "polymer-3"
    ]
  }
}